#include <stdio.h>

int main()
{
    int i=0,k=1,j;
    int a[6]={100,90,90,80,75,60};
    int b[6]={0};
    int c[5]={50,65,77,90,102};
    int d[5];
    
    for(i=0;i<6;i++){
        if(i==0){
            b[i]=1;
        }
        else if(a[i]==a[i-1]){
            b[i]=b[i-1];
        }else if(a[i]<a[i-1]){
            b[i]=1+b[i-1];
        }
    }
    for(i=0;i<5;i++){
        for(j=0;j<6;j++){
            if(a[j]<c[i]){
                d[i]=b[j];
                break;
            }
            else if(a[j]>c[i]){
                while(j!=6 && a[j]>c[i]){
                    j++;
                
                }
                d[i]=1+b[j-1];
                break;
            }
        }
    }
    for(i=0;i<5;i++){
        printf("%d",d[i]);
    }
    return 0;
}
